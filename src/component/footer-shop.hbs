<div class="row fs-5">
   <div class="col-2">
      <ul class="nav flex-column">
         {{#each footer.[0]}}
         <li class="nav-item">
            <a class="nav-link {{#if @last}}disabled{{/if}}" aria-current="page" href="{{this.link}}">{{this.text}}</a>
