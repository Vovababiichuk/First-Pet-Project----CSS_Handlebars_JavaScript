{{#with web}}
<main>
   <section class="web">
      <div class="container">
         <div class="web__html">
            <div class="web__html-title">
               <img class="web__html-img" src="/images/html.png" alt="html photo" width="200" height="200">
               <div>
                  {{#with languages.[0]}}
                     <span class="web__html-name">Name: {{name}}</span>
                     <span>Version: {{version}}</span>
                     <span>Description: {{description}}</span>
                  {{/with}}
               </div>
            </div>
            <div class="web__html-elements">
               <span>{{> title data="Elements" big=true}}</span>
               <mark>{{languages.[0].elements.[0].name}}</mark>
               <div class="web__html-attributes">
                  <span>description: {{languages.[0].elements.[0].description}}</span>
                  <span>Attributes:</span>
                  <ul>
                     {{#with languages.[0].elements.[0]}}
                        <li>{{attributes.[0].name}}</li>
                        <span>Description: {{attributes.[0].description}}</span>
                        <li>{{attributes.[1].name}}</li>
                        <span>Description: {{attributes.[1].description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>
            <div class="web__html-elements">
               <mark>{{languages.[0].elements.[1].name}}</mark>
               <div class="web__html-attributes">
                  <span>description: {{languages.[0].elements.[1].description}}</span>
                  <span>Attributes:</span>
                  <ul>
                     {{#with languages.[0].elements.[1]}}
                        <li>{{attributes.[0].name}}</li>
                        <span>Description: {{attributes.[0].description}}</span>
                        <li>{{attributes.[1].name}}</li>
                        <span>Description: {{attributes.[1].description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>
         </div>
         <div class="web__css">
            <div class="web__css-title">
               <img class="web__css-img" src="/images/css.png" alt="css photo" width="200" height="200">
               <div>
                  {{#with languages.[1]}}
                     <span class="web__css-name">Name: {{name}}</span>
                     <span>Version: {{version}}</span>
                     <span>Description: {{description}}</span>
                  {{/with}}
               </div>
            </div>
            <div class="web__css-properties">
               <span>{{> title data="Properties" big=true}}</span>
               <mark>{{languages.[1].properties.[0].name}}</mark>
               <div class="web__css-attributes">
                  <span>description: {{languages.[1].properties.[0].description}}</span>
                  <span>Values:</span>
                  <ul>
                     {{#with languages.[1].properties.[0]}}
                        <li>{{values.[0].value}}</li>
                        <span>Description: {{values.[0].description}}</span>
                        <li>{{values.[1].value}}</li>
                        <span>Description: {{values.[1].description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>
            <div class="web__css-elements">
               <mark>{{languages.[1].properties.[1].name}}</mark>
               <div class="web__css-attributes">
                  <span>description: {{languages.[1].properties.[1].description}}</span>
                  <span>Values:</span>
                  <ul>
                     {{#with languages.[1].properties.[1]}}
                        <li>{{values.[0].value}}</li>
                        <span>Description: {{values.[0].description}}</span>
                        <li>{{values.[1].value}}</li>
                        <span>Description: {{values.[1].description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>
         </div>
         <div class="web__js">
            <div class="web__js-title">
               <img class="web__js-img" src="/images/js.png" alt="css photo" width="180" height="180">
               <div>
                  {{#with languages.[2]}}
                     <span class="web__js-name">Name: {{name}}</span>
                     <span>Version: {{version}}</span>
                     <span>Description: {{description}}</span>
                  {{/with}}
               </div>
            </div>
            <div class="web__js-functions">
               <span>{{> title data="Functions" big=true}}</span>
               <mark>{{languages.[2].functions.[0].name}}</mark>
               <div class="web__css-attributes">
                  <span>description: {{languages.[2].functions.[0].description}}</span>
                  <span>Parameters:</span>
                  <ul>
                     {{#with languages.[2].functions.[0].parameters.[0]}}
                        <li>{{name}}</li>
                        <li>Type: {{type}}</li>
                        <span>Description: {{description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>
            <div class="web__js-functions">
               <mark>{{languages.[2].functions.[1].name}}</mark>
               <div class="web__css-attributes">
                  <span>description: {{languages.[2].functions.[1].description}}</span>
                  <span>Parameters:</span>
                  <ul>
                     {{#with languages.[2].functions.[1].parameters.[0]}}
                        <li>{{name}}</li>
                        <li>Type: {{type}}</li>
                        <span>Description: {{description}}</span>
                     {{/with}}
                  </ul>
               </div>
            </div>            
         </div>        
      </div>
   </section>
</main>
{{/with}}