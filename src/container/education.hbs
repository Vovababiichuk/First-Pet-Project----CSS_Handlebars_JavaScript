<div>
   {{#with main}}
      <main>
         <section>
            {{> title data="My education" big=true}}
            <ul>
               {{#each educations}}
                  <li>
                     {{name}}
                     {{#if isEnd}} (ще в процесі) {{/if}}
                  </li>
               {{/each}}
            </ul>
         </section>
         <section>
            {{> title data="My Certificates"}}
            <ul>
               {{#each certificates}}
                  <li>
                     {{name}} ({{id}})
                  </li>
               {{/each}}
            </ul>
         </section>
      </main>
   {{/with}}
</div>

